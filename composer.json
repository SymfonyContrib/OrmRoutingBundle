{
    "name": "symfonycontrib/orm-routing-bundle",
    "type": "symfony-bundle",
    "description": "Symfony routing stored via Doctrine ORM based on the Symfony CMF components and bundles.",
    "keywords": ["routing", "database", "ORM"],
    "homepage": "http://symfonycontrib.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/RoutingBundle/contributors"
        },
        {
            "name": "Alan Doucette",
            "email": "dragonwize@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony-cmf/routing":      "^1.4",
        "symfony-cmf/routing-auto": "dev-master#54b53a1",
        "aferrandini/urlizer":      "1.0.*"
    },
    "require-dev": {
        "symfony/monolog-bundle":  "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "symfony-cmf/routing-bundle":      "*",
        "symfony-cmf/routing-auto-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "SymfonyContrib\\Bundle\\OrmRoutingBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
